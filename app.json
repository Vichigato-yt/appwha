{
  "expo": {
    "name": "appa3",
    "slug": "appa3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconoapp.jpg",
    "scheme": "appa3",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "backgroundImage": "./assets/images/iconoapp.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/iconoapp.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Permite acceso para fotos.",
          "microphonePermission": "Permite acceso para video."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permite guardar fotos en tu galería.",
          "savePhotosPermission": "Permite guardar fotos en tu galería.",
          "isAccessMediaLocationEnabled": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconoapp.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
